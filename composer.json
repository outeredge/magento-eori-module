{
    "name": "outeredge/magento-eori-module",
    "description": "Magento Eori Module by outer/edge",
    "type": "magento2-module",
    "version": "1.0.1",
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "outer/edge",
            "email": "support@outeredge.agency",
            "homepage": "https://outeredge.agency/",
            "role": "Developer"
        }
