    "require": {
        "php": "^8.1",
        "davidvandertuijn/eori": "2.0.0"
    },
    "require-dev": {
        "outeredge/dev-dependencies": "^3.0"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "OuterEdge\\Eori\\": ""
        }
